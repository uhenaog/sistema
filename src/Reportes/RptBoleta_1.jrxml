	<queryString>
		<![CDATA[SELECT
(select nombre_empresa from datos_empresa )as nombre_empresa,
(select rut_empresa from datos_empresa )as rut_empresa,
v.cod_venta , v.fecha_venta, v.total_venta, v.pago , v.descuento , (SELECT p.nombre_persona FROM persona p WHERE v.cod_usuarioFK = p.cod_persona )AS NombreUsuario ,(SELECT p.nombre_persona FROM persona p WHERE v.cod_clienteFK = p.cod_persona ) AS nombreCliente ,d.cod_productoFK,(SELECT p.nombre_producto FROM producto p WHERE p.cod_producto = d.cod_productoFK)AS NombreProducto, (SELECT SUM(d.cantidad_detalle))as totalCantidad , (SELECT SUM(d.subtotal))as SubTotalProd,d.precio_producto,metodo_pago,nomCaja FROM venta v INNER JOIN detalle_venta d ON v.cod_venta = d.cod_ventaFK WHERE v.cod_venta = $P{cod_venta}  AND v.tipo_comprobante ='Boleta' GROUP by d.cod_productoFK ASC]]>
